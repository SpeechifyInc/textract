{
  "name": "@speechifyinc/textract",
  "version": "2.5.3",
  "type": "module",
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com"
  },
  "bin": {
    "textract": "./bin/textract"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "test": "vitest",
    "lint": "eslint \"{lib,bin,scripts,test}/**/*.{js,ts}\"",
    "lint:fix": "eslint --fix \"{lib,bin,scripts,test}/**/*.{js,ts}\"",
    "format": "prettier --write \"{lib,bin,scripts,test}/**/*.{js,ts}\""
  },
  "author": "David Bashford",
  "description": "Extracting text from files of various type including html, pdf, doc, docx, xls, xlsx, csv, pptx, png, jpg, gif, rtf, text/*, and various open office.",
  "contributors": [
    {
      "name": "David Bashford",
      "email": "dbashford@hotmail.com"
    }
  ],
  "dependencies": {
    "cheerio": "^1.1.2",
    "epub2": "1.3.4",
    "got": "5.7.1",
    "html-entities": "1.2.0",
    "iconv-lite": "0.4.15",
    "j": "0.4.3",
    "jschardet": "1.4.1",
    "marked": "0.6.2",
    "meow": "3.7.0",
    "mime": "2.2.0",
    "pdf-text-extract": "1.3.1",
    "xmldom": "0.1.27",
    "xpath": "0.0.23",
    "yauzl": "2.7.0"
  },
  "devDependencies": {
    "@speechifyinc/platform-code-conformity-kit": "^3.0.0",
    "@types/meow": "^6.0.0",
    "@types/node": "^22.18.13",
    "@types/yauzl": "^2.10.3",
    "domhandler": "^5.0.3",
    "eslint": "^9.21.0",
    "prettier": "^3.6.2",
    "vitest": "^4.0.6"
  },
  "license": "MIT",
  "engines": {
    "node": ">=22"
  },
  "volta": {
    "node": "22.14.0",
    "pnpm": "10.20.0"
  },
  "packageManager": "pnpm@10.20.0"
}
